{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "rules": {
    "react/forbid-prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "import/prefer-default-export": 0
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "globals": {
    "tw": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "plugins": ["prettier"]
}
